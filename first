#include <stdio.h>

int main(void) {
    int t;
    scanf("%d",&t);
    while(t--){
        int n,k;
        scanf("%d %d",&n,&k);
        if(n%4 == 0)
            k == 0 ? printf("Off\n") : printf("On\n");
        else if(k == 0)
            printf("On\n");
        else
            printf("Ambiguous\n");
    }
	return 0;
}

